# Configuração do Ambiente
APP_ENV=production
APP_DEBUG=false
APP_URL=https://godeliverypro.com

# Configuração do Banco de Dados EXTERNO
# Configure aqui as credenciais do seu banco de dados externo
DB_HOST=seu-servidor-mysql.com
DB_PORT=3306
DB_DATABASE=nome_do_banco
DB_USERNAME=usuario_do_banco
DB_PASSWORD=senha_do_banco

# Configuração do Nginx (portas)
NGINX_HTTP_PORT=80
NGINX_HTTPS_PORT=443

# Trusted Proxies (importante para capturar IP real)
TRUSTED_PROXIES=*
